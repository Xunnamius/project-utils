    "remark-reference-links": "^6.0.1",
    "remark-remove-comments": "^1.0.1",
    "remark-remove-unused-definitions": "^1.0.4",
    "remark-remove-url-trailing-slash": "^1.0.2",
    "remark-renumber-references": "^1.0.5",
    "remark-sort-definitions": "^1.0.5",
