    "babel-plugin-transform-rewrite-imports": "^1.1.0",
    "confusing-browser-globals": "^1.0.11",
    "conventional-changelog-cli": "https://xunn.at/conventional-changelog-cli",
    "doctoc": "^2.2.1",
