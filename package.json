{
  "name": "this-package-needs-a-name",
  "version": "0.0.1",
  "description": "<THIS PACKAGE NEEDS A DESCRIPTION>",
  "repository": {
    "type": "git",
    "url": "<THIS URL NEEDS TO BE UPDATED>"
  },
  "license": "Unlicense",
  "author": "Xunnamius",
  "scripts": {
    "build": "next build",
    "bump-major": "npx bump --major --tag --commit --lock --all",
    "bump-minor": "npx bump --minor --tag --commit --lock --all",
    "bump-patch": "npx bump --patch --tag --commit --lock --all",
    "clean-types": "npx gulp cleanTypes",
    "debug-remote": "node --inspect-brk='0.0.0.0:7777'",
    "debug-remote-gulp": "node --inspect-brk='0.0.0.0:7777' ./node_modules/.bin/gulp",
    "debug-remote-jest": "node --inspect-brk='0.0.0.0:7777' ./node_modules/.bin/jest --runInBand",
    "dev": "next",
    "generate": "BABEL_ENV=generator npx babel config --out-dir .",
    "init": "npx gulp eject",
    "install-types": "npx flow-typed install",
    "list-tasks": "npm run && echo 'Compiling gulp task list...' && npx gulp --tasks",
    "regenerate": "npx gulp regenerate",
    "start": "next start",
    "test": "npx jest",
    "test-ib": "npx jest --runInBand",
    "test-nc": "npx jest --no-cache",
    "preversion": "npm run test-ib",
    "version": "git add --all"
  },
  "jest": {
    "moduleNameMapper": {
      "^universe$": "<rootDir>/src",
      "^universe/(.*)$": "<rootDir>/src/$1",
      "^components/(.*)$": "<rootDir>/src/components/$1"
    },
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/jest.preprocess.js"
    },
    "verbose": true
  },
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autobind-decorator": "^2.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "bulma": "^0.9.1",
    "confusing-browser-globals": "^1.0.7",
    "dotenv": "^8.0.0",
    "immutable": "^4.0.0-rc.12",
    "next": "^9.3.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-motion": "^0.5.2",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1",
    "styled-jsx": "^3.2.1"
  },
  "devDependencies": {
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-source-map-support": "^2.0.1",
    "chalk": "^2.4.2",
    "del": "^4.1.1",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "^3.9.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "fancy-log": "^1.3.3",
    "flow-bin": "^0.98.1",
    "flow-typed": "^2.5.2",
    "formik": "^1.5.7",
    "gulp": "^4.0.2",
    "gulp-tap": "^1.0.1",
    "inquirer": "^6.3.1",
    "jest": "^24.8.0",
    "parse-gitignore": "^1.0.1",
    "replace-in-file": "^4.1.0",
    "shelljs": "^0.8.3",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.12",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "version-bump-prompt": "^5.0.1",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "expectedEnvVariables": [
    "BUNDLE_ANALYZE",
    "NODE_ENV|BABEL_ENV|APP_ENV"
  ]
}
